.fr_node_on {
    width: 660px;
    margin: 6px 0 6px 3px;
    background: #222;
    float: left;
}
.fr_node_off {
    width: 50px;
    margin: 2px;
    float: left;
}
.fr_img {
    width: 50px;
    height: 50px;
    float: left;
}
.fr_nodecn_on {
    display: block;
    width: 600px;
    margin: 0 0 0 3px;
    float: left;
}
.fr_nodecn_off {
    display: none;
}
.fr_title_on {
    display: block;
    background-color: #444;
    border-bottom: solid 1px #008000;
    width: 100%;
    vertical-align: top;
    padding: 0pt;
}
.fr_title_off {
    display: none;
}
.fr_cn_on {
    display: block;
}
.fr_cn_off {
    display: none;
}
.fr_hr_on {
    clear: left;
    display: inline;
    color: #000;
    background: #000;
}
.fr_hr_off {
    display: none;
}